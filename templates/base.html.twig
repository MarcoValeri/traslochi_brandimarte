<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
        <header class="header">
            {# Navbar #}
            <nav class="header-navbar">
                <section class="header-navbar-menu-toggle">
                    <input type="checkbox" />
                    <span></span>
                    <span></span>
                    <span></span>
                    <ul class="header-navbar-menu">
                        <li>Home</li>
                        <li>Chi siamo</li>
                        <li>Servizi</li>
                        <li>Preventivo gratuito</li>
                        <li>Contatti</li>
                    </ul>
                </section>
            </nav>
        </header>
        {% block body %}{% endblock %}
    </body>
</html>
